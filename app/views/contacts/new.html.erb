<div class="container">
  <%= form_tag "/contacts", method: :post do %>
  <div>
    <h4>
      <%= label_tag :first_name %>
      <%= text_field_tag :first_name %>
    </h4>
  </div>
  <div>
    <h4>
      <%= label_tag :middle_name %>
      <%= text_field_tag :middle_name %>
    </h4>
  </div>
 <div>
    <h4>
      <%= label_tag :last_name %>
      <%= text_field_tag :last_name %>
    </h4>
  </div>
 <div>
    <h4>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </h4>
  </div>
 <div>
    <h4>
      <%= label_tag :phone_number %>
      <%= text_field_tag :phone_number %>
    </h4>
  </div>
  <div>
    <h4>
      <%= label_tag :address %>
      <%= text_field_tag :address %>
    </h4>
  </div>
 <div>
    <h4>
      <%= label_tag :bio %>
      <%= text_field_tag :bio %>
    </h4>
  </div>
  <div>
    <%= submit_tag "Add Contact" %>
  </div>
  <% end %>
</div>